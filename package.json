{
  "name": "dove-text",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:init": "NODE_ENV=development ts-node --project scripts/tsconfig.json scripts/init-db.ts",
    "db:create": "NODE_ENV=development ts-node --project scripts/tsconfig.json scripts/create-db.ts",
    "db:init:vercel": "NODE_ENV=production ts-node --project scripts/tsconfig.json scripts/init-vercel-db.ts",
    "create-code": "tsx src/scripts/createInvitationCode.ts",
    "create:code": "NODE_ENV=development ts-node --project scripts/tsconfig.json scripts/create-invitation-code.ts",
    "db:migrate": "ts-node --project scripts/tsconfig.json scripts/db/migrate.ts",
    "db:migrate:create": "node -e \"require('fs').writeFileSync(`scripts/db/migrations/${Date.now()}_${process.argv[1]}.ts`, require('fs').readFileSync('scripts/db/migration.template.ts'))\"",
    "db:migrate:status": "ts-node --project scripts/tsconfig.json scripts/db/migrate.ts status"
  },
